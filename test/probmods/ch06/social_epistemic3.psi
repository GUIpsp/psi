def haveCookie(obj) ⇒ obj==cookie;

def dirichlet(α: ℝ[]){ // TODO: add dirichlet distribution
	assert(α.length==2&&α[0]==1&&α[1]==1);
	p := uniform(0,1);
	return [p,1-p];
}

