b := 6;
def bsearch(x){    
    o := 0;
    m := 0;
    for i in [0..b){
        m = 2^(b-i-1);
        if o + m <= x{
            o += m;
        }
    }
    return o;
}

def main(){
    return bsearch(uniformInt(0,2^b-1));
}

